@Library('my-shared-lib') _


pipeline{
    agent any 

    stages{
            stage("Git Checkout"){
            
            steps{
                gitCheckout(
                    branch: "main",
                    url: "https://github.com/chanakyaboyini/DevOps_Jenkins.git"
            )
   
        }
        }
        agent DevOps_Node
        stage("Unit testing Maven"){
        
        steps{
             script{
                mvnTest()
             }
        }
}
    }
}

